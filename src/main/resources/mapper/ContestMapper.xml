<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo1.dao.ContestMapper">
    <resultMap id="BaseContestMap" type="com.example.demo1.entity.Contest">
        <id column="id" property="id"/>
        <result column="contest_name" property="contest_name"/>
        <result column="intro" property="intro"/>
        <result column="code" property="code"/>
    </resultMap>

    <insert id="insertContest" useGeneratedKeys="true">
        insert into contest(id, contest_name, intro, code)
        values (#{id}, #{contest_name}, #{intro}, #{code})
    </insert>

    <delete id="deleteContest">
        delete from contest
        where id = #{id}
    </delete>

</mapper>