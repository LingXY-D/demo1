<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo1.dao.ContestMapper">
    <resultMap id="BaseContestMap" type="com.example.demo1.entity.Contest">
        <id column="guid" property="guid"/>
        <result column="level_num" property="level_num"/>
        <result column="start_time" property="start_time"/>
        <result column="limit_time" property="limit_time"/>
        <result column="contest_name" property="contest_name"/>
    </resultMap>

    <insert id="insertContest" useGeneratedKeys="true">
        insert into contest(guid, level_num, start_time, limit_time, contest_name)
        values (#{guid}, #{level_num}, #{start_time}, #{limit_time}, #{contest_name})
    </insert>

</mapper>